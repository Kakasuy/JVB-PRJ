# Amadeus API Configuration
# Get your API credentials from https://developers.amadeus.com/
AMADEUS_CLIENT_ID=your_amadeus_client_id
AMADEUS_CLIENT_SECRET=your_amadeus_client_secret

# Google Maps Configuration
NEXT_PUBLIC_GOOGLE_MAP_API_KEY=your_google_map_api_key
# https://visgl.github.io/react-google-maps/docs/get-started

NEXT_PUBLIC_GOOGLE_MAP_ID=your_google_map_id
# https://visgl.github.io/react-google-maps/docs/api-reference/components/advanced-marker

# Firebase Configuration
# Get these from Firebase Console > Project Settings > General > Your apps
NEXT_PUBLIC_FIREBASE_API_KEY=your_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id_here
